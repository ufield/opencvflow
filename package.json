    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "electron": "^15.5.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
